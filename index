	</form>
	<script>
		var input = prompt("What is your input?");
		var kplresult;
		var mpgresult;
		

		//onclick "converttoKPLbutton" = mpgtokpl(input)
		function mpgtokpl(mpginput)
		{
			kplresult = input * 0.425144;
		}
		

		//onclick "converttoMPGbutton" = mpgtokpl(input);
		function kpltompg(kplinput)
		{
			mpgresult = input * 2.35215;
		}
	</script>
